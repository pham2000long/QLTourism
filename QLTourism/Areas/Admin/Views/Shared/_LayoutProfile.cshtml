<!DOCTYPE html>
<html>
<head>
    <title>@Page.Title</title>
    <link rel="shortcut icon" href="~/Areas/Admin/Public/assets/images/favicon.ico">

    <link href="~/Areas/Admin/Public/assets/libs/dropify/css/dropify.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Areas/Admin/Public/assets/libs/magnific-popup/magnific-popup.css" rel="stylesheet" type="text/css" />

    <!-- Bootstrap Css -->
    <link href="~/Areas/Admin/Public/assets/css/bootstrap.min.css" id="bootstrap-style" rel="stylesheet" type="text/css" />
    <!-- Icons Css -->
    <link href="~/Areas/Admin/Public/assets/css/icons.min.css" rel="stylesheet" type="text/css" />
    <!-- App Css-->
    <link href="~/Areas/Admin/Public/assets/css/app.min.css" id="app-style" rel="stylesheet" type="text/css" />
    @RenderSection("head", required: false)
</head>
<body>
    @RenderBody()
    <!-- JAVASCRIPT -->
    <script src="~/Areas/Admin/Public/assets/libs/jquery/jquery.min.js"></script>
    <script src="~/Areas/Admin/Public/assets/libs/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/Areas/Admin/Public/assets/libs/metismenu/metisMenu.min.js"></script>
    <script src="~/Areas/Admin/Public/assets/libs/simplebar/simplebar.min.js"></script>
    <script src="~/Areas/Admin/Public/assets/libs/node-waves/waves.min.js"></script>

    <!-- google maps api -->
    <script src="https://maps.google.com/maps/api/js?key=AIzaSyCtSAR45TFgZjOs4nBFFZnII-6mMHLfSYI"></script>

    <!-- Gmaps file -->
    <script src="~/Areas/Admin/Public/assets/libs/gmaps/gmaps.min.js"></script>

    <script src="~/Areas/Admin/Public/assets/libs/dropify/js/dropify.min.js"></script>

    <!-- Magnific Popup-->
    <script src="~/Areas/Admin/Public/assets/libs/magnific-popup/jquery.magnific-popup.min.js"></script>

    <!-- Chart JS -->
    <script src="~/Areas/Admin/Public/assets/libs/chart.js/Chart.bundle.min.js"></script>

    <script src="~/Areas/Admin/Public/assets/js/pages/profile.init.js"></script>

    <script src="~/Areas/Admin/Public/assets/js/app.js"></script>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
